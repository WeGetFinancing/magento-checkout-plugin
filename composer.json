{
    "name": "wegetfinancing/magento-checkout-plugin",
    "version": "1.0.0",
    "type": "magento2-module",
    "description": "WeGetFinancing checkout module for Magento 2.",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "Riccardo De Leo",
            "email": "riccardo.deleo@empaytech.com"
        }
    ],
    "require": {
        "magento/module-payment": "100.*",
        "wegetfinancing/php-sdk": "2.*",
        "sentry/sdk": "^3.5"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "WeGetFinancing\\Checkout\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true
        }
    }
}
